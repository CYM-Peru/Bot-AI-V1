{
  "name": "flow-builder-demo",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "tsc -b && vite build",
    "build:server": "echo 'Server uses runtime tsx - no build needed'",
    "build:server:legacy": "tsc -p tsconfig.server.build.json",
    "start:server": "tsx server/index.ts",
    "init-admin": "tsx server/init-admin.js",
    "preview": "vite preview --port 5173",
    "test": "vitest run",
    "test:ci": "vitest run --coverage",
    "test:integration": "vitest run --dir tests/integration",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "depcheck": "depcheck",
    "ts-prune": "ts-prune",
    "tsc": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@xyflow/react": "^12.2.4",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "chart.js": "^4.5.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.0",
    "html-to-image": "^1.11.13",
    "https-proxy-agent": "^7.0.6",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.1",
    "undici": "^7.16.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "ts-prune": "^0.10.3",
    "tsx": "^4.7.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vitest": "^1.6.0"
  }
}
